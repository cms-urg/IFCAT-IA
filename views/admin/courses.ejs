    <thead>
        <tr>
            <th>Code</th>
            <th>Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    <% courses.forEach(function (course) { %>
        <tr>
            <th><%= course.code %></th>
            <th><%= course.name %></th>
            <th>

            </th>
        </tr>
    <% }) %>
    </tbody>
</table>

