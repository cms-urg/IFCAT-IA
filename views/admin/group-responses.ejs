<%- include ./header -%>

<h1>Responses</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th style="width: 10px;">No.</th>
            <th style="max-width: 250px;">Question</th>
            <th class="text-center"># of attempts</th>
            <th class="text-center">Points awarded</th>
        </tr>
    </thead>
    <tfoot>
        <tr class="total-points">
            <th colspan="3">Total Points</th>
            <th></th>
        </tr>
    </tfoot>
    <tbody>
    <% for (var i = 0, len = responses.length; i < len; i++) { %>
        <tr>
            <td style="width: 10px;"><%= responses[i].question.number %>.</td>
            <td style="max-width: 250px;"><%= responses[i].question.question %></td>
            <td class="text-center"><%= responses[i].attempts %></td>
            <td class="text-center"><%= responses[i].points %></td>
            <td class="text-center"></td>
        </tr>
    <% } %>
    </tbody>
</table>

<%- include ./footer -%>